<Window x:Class="_30_09_2022.Prompt"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_30_09_2022"
        ResizeMode="NoResize"
        Icon="/Images/files_edit.ico"
        mc:Ignorable="d"
        Title="Prompt" Height="250" Width="400">
    <Canvas>
        <TextBlock Text="New tab name" Canvas.Top="90" Canvas.Left="40"/>
        <TextBox x:Name="name" Canvas.Top="90" Canvas.Right="90" Width="150"/>
        <Button Content="Create" Height="20" Width="80" Canvas.Bottom="30" Canvas.Left="30" Click="Create_Click"/>
        <Button Content="Close" Height="20" Width="80" Canvas.Bottom="30" Canvas.Right="30" Click="Close_Click"/>
    </Canvas>
</Window>
